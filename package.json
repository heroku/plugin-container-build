{
  "name": "@heroku-cli/plugin-local-build",
  "description": "Use this plugin to build and run your app locally as a docker image/container.",
  "version": "0.1.9",
  "author": "Joe Kutner @codefinger",
  "bugs": "https://github.com/heroku/plugin-local-build/issues",
  "dependencies": {
    "@heroku-cli/command": "^8.1.23",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@types/debug": "^0.0.30",
    "@types/shell-escape": "^0.2.0",
    "cli-ux": "^4.6.3",
    "debug": "^3.1.0",
    "execa": "^0.10.0",
    "node-fetch": "^2.1.2",
    "tslib": "^1"
  },
  "devDependencies": {
    "@heroku-cli/schema": "^1.0.5",
    "@oclif/dev-cli": "^1",
    "@oclif/plugin-help": "^2",
    "@oclif/test": "^1",
    "@oclif/tslint": "^1",
    "@types/chai": "^4",
    "@types/execa": "^0.9.0",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "globby": "^8",
    "mocha": "^5",
    "nyc": "^11",
    "ts-node": "^6",
    "tslint": "^5",
    "typescript": "^2.9"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/oclif.manifest.json",
    "/yarn.lock",
    "/scripts"
  ],
  "homepage": "https://github.com/heroku/plugin-local-build",
  "keywords": [
    "oclif-plugin"
  ],
  "license": "MIT",
  "tatara": {
    "version": "0.1.7"
  },
  "oclif": {
    "commands": "./lib/commands",
    "bin": "oclif-example",
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "heroku/plugin-local-build",
  "scripts": {
    "postinstall": "node scripts/download.js",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc && oclif-dev manifest && oclif-dev readme",
    "prepare": "rm -rf lib && tsc",
    "test": "nyc mocha --forbid-only \"test/**/*.test.ts\" --timeout 50000",
    "posttest": "tslint -p .",
    "version": "oclif-dev readme && git add README.md"
  }
}
